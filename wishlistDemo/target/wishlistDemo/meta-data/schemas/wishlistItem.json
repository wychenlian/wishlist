{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Wishlist Item",
  "type" : "object",
  "properties":
  {
    "productId":
    {
      "type":"string",
      "pattern":"^.+"
    },
    "productName":
    {
      "type":"string",
      "pattern":"^.+"
    },
    "priceId":
    {
      "type":"string",
      "pattern":"^.+"
    },
    "price":
    {
      "type":"string",
      "pattern":"^.+"
    },
    "amount":
    {
      "type":"integer",
      "minimum":1,
      "default":1
    },
    "createdAt":
    {
      "type":"string",
      "format":"date-time"
    }
  }
}
